{
    "name": "windwalker/starter",
    "description": "Windwalker Starter.",
    "homepage": "https://github.com/windwalker-io/starter",
    "license": "MIT",
    "minimum-stability": "beta",
    "authors": [
        {
            "name": "Simon Asika",
            "email": "asika32764@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "windwalker/core": "^4.0",
        "windwalker/renderer": "^4.0",
        "windwalker/dom": "^4.0",
        "windwalker/edge": "^4.0",
        "asika/cross-env": "^1.0",
        "symfony/yaml": "^5.4",
        "symfony/var-dumper": "^5.4",
        "react/event-loop": "dev-master as 1.2",
        "react/filesystem": "dev-master as 0.1.2",
        "wyrihaximus/react-child-process-pool": "dev-master as 1.8.0",
        "wyrihaximus/react-child-process-messenger": "dev-master as 4.0.1",
        "react/stream": "^1.2",
        "erusev/parsedown": "^1.7@stable",
        "erusev/parsedown-extra": "^0.8"
    },
    "require-dev": {
        "windwalker/framework": "dev-master"
    },
    "scripts": {
        "post-autoload-dump": []
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "config": {
        "platform": {
            "php": "8.0.0"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev",
            "next": "4.x-dev"
        }
    }
}

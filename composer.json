{
    "name": "asika/vaseman",
    "description": "Yet another PHP static site generator.",
    "keywords": ["static site", "prototype", "windwalker", "static pages"],
    "homepage": "https://github.com/asika32764/vaseman",
    "license": "MIT",
    "minimum-stability": "beta",
    "authors": [
        {
            "name": "Simon Asika",
            "email": "asika32764@gmail.com"
        }
    ],
    "require": {
        "php": ">=8.0",
        "ext-json": "*",
        "windwalker/core": "^4.0",
        "windwalker/renderer": "^4.0",
        "windwalker/dom": "^4.0",
        "windwalker/edge": "^4.0",
        "asika/cross-env": "^1.0",
        "symfony/yaml": "^5.4||^6.0||^7.0",
        "symfony/var-dumper": "^5.4||^6.0||^7.0",
        "erusev/parsedown": "^1.7@stable",
        "erusev/parsedown-extra": "^0.8"
    },
    "require-dev": {
        "windwalker/framework": "dev-master"
    },
    "scripts": {
        "post-autoload-dump": []
    },
    "autoload": {
        "psr-4": {
            "App\\": "src/"
        },
        "files": [
            "src/bootstrap.php"
        ]
    },
    "config": {
        "platform": {
            "php": "8.2.0"
        },
        "allow-plugins": {
            "wyrihaximus/composer-update-bin-autoload-path": true
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "4.x-dev",
            "next": "4.x-dev"
        }
    },
    "bin": [
        "vaseman"
    ]
}
